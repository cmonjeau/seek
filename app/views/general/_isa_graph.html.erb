<%
  include_parents ||= false
  deep ||= false
  depth ||= 1
  hash =  Seek::IsaGraphGenerator.new(root_item).generate({ deep: deep, depth: depth, include_parents: include_parents })
  elements = cytoscape_elements(hash)

  #show only current node and connected nodes
  #if set force_max_node to true, show only max_node_number if (current node + connected nodes) is bigger then max_node_number
  # reduced_elements = reduced_elements(elements, max_node_number, force_max_node, root_item)
%>

<%= render partial: 'general/cytoscape_graph', locals: { elements: elements, root_item: root_item, hash: hash } %>
